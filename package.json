    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@uiw/codemirror-theme-github": "^4.25.4",
    "codemirror": "^6.0.2",
    "maplibre-gl": "^5.12.0",
    "oidc-client-ts": "^3.4.1",
    "polished": "^4.3.1",
    "pyodide": "^0.29.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-map-gl": "^8.1.0",
