{
  "name": "openeo-studio",
  "description": "Interactive code editor for openEO satellite imagery processing",
  "version": "0.1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/developmentseed/openeo-studio.git"
  },
  "author": {
    "name": "Development Seed",
    "url": "https://developmentseed.org"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/developmentseed/openeo-studio/issues"
  },
  "homepage": "https://github.com/developmentseed/openeo-studio",
  "scripts": {
    "prepare": "husky",
    "dev": "pnpm clean && NODE_ENV=development vite",
    "build": "pnpm clean && NODE_ENV=production tsc -b && vite build",
    "stage": "pnpm clean && NODE_ENV=staging tsc -b && vite build",
    "clean": "rm -rf dist node_modules/.vite",
    "lint": "pnpm lint:scripts",
    "lint:scripts": "eslint app/",
    "ts-check": "npx tsc --noEmit --skipLibCheck",
    "test": "jest",
    "test:integration": "playwright test",
    "test:integration:ui": "playwright test --ui",
    "test:integration:debug": "playwright test --debug",
    "test:integration:headed": "playwright test --headed"
  },
  "engines": {
    "node": "22.x"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.58.0",
    "@types/babel__core": "^7",
    "@types/jest": "^30.0.0",
